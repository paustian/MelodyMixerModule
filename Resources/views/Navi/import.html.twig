<fieldset>
    <h3>
        <span class="fa fa-edit"></span>
        {{ "Import Data for Levels" | trans }}
        {{ pageSetVar('title', 'Import Level Data'|trans) }}
    </h3>
    {{ showflashes() }}
    {{ form_start(form) }}
    {{ form_errors(form) }}
    <fieldset>
        <legend>{{ "Upload MelodyMixer Data"|trans }}</legend>
        {{ form_row(form.import) }}
        <div class="col-lg-offset-3 col-lg-9">
            {{ form_widget(form.submit, {'attr': {'class': 'btn btn-success'}}) }}
        </div>
    </fieldset>
    {{ form_end(form) }}
</div>