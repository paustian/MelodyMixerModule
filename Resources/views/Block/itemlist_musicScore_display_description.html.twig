{# purpose of this template: Display music scores within an external context #}
{% trans_default_domain 'musicScore' %}
<dl>
    {% for musicScore in items %}
        <dt>{{ musicScore|paustianmelodymixermodule_formattedTitle }}</dt>
        {% if musicScore.gsGraphic %}
            <dd>{{ musicScore.gsGraphic|striptags|u.truncate(200, 'â€¦') }}</dd>
        {% endif %}
    {% else %}
        <dt>{% trans %}No music scores found.{% endtrans %}</dt>
    {% endfor %}
</dl>
