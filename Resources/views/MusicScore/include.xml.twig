{# purpose of this template: music scores xml inclusion template #}
{% trans_default_domain 'musicScore' %}
<musicscore id="{{ musicScore.getId() }}" createdon="{{ musicScore.createdDate|format_datetime('medium', 'short') }}" updatedon="{{ musicScore.updatedDate|format_datetime('medium', 'short') }}">
    <id>{{ musicScore.id }}</id>
    <levelId>{{ musicScore.levelId }}</levelId>
    <gsGraphic><![CDATA[{{ musicScore.gsGraphic }}]]></gsGraphic>
    <gsMidi><![CDATA[{{ musicScore.gsMidi }}]]></gsMidi>
    <scoreIt>{{ musicScore.scoreIt }}</scoreIt>
    <musicOrder>{{ musicScore.musicOrder }}</musicOrder>
</musicscore>
